tStreamNamed: props allClassCategoryIndexName , props htmlSuffix.	self writeAllClassCategoryIndex.	str close.! !!DlHtmlOutputter methodsFor: 'actions-output' stamp: 'M.U 5/3/1999 18:51'!outputAllClassIndex	| props str |	props := self properties.	str := self outStreamNamed: props allClassIndexName, props htmlSuffix.	self writeAllClassIndex.	str close.	! !!DlHtmlOutputter methodsFor: 'actions-output' stamp: 'MU 6/25/2000 01:05'!outputClassCategories	| props str nm |	props := self properties.	self analysisStorage getAllClassCategories do: [:each | (each isResolved or: [props outputResolvedOnly not])			ifTrue: 				[nm := each name.				str := self outStreamNamed: props classCategorySummaryName , props htmlSuffix in: (Array with: nm).				self writeClassCategorySummaryFor: each.				str close.				str := self outStreamNamed: props classCategoryDetailName , props htmlSuffix in: (Array with: nm).				self writeClassCategoryDetailFor: each.				str close]].! !!DlHtmlOutputter methodsFor: 'actions-output' stamp: 'mu 1/3/2001 22:45'!outputClasses	| props |	props := self properties.	self analysisStorage getAllClasses do: [:each | (each isResolved or: [props outputResolvedOnly not])			ifTrue: 				[| nm cat | 				nm := each name.				cat := each category.				cat isNil					ifFalse: 						[| str | 						str := self outStreamNamed: nm , props htmlSuffix in: (Array with: cat name).						self writeClassFor: each.						str close]]]! !!DlHtmlOutputter methodsFor: 'actions-output' stamp: 'M.U 12/27/1999 00:33'!outputGlobalRoots	| props str |	props := self properties.	str := self outStreamNamed: (props generalIndexName, props htmlSuffix) in: (Array with: props globalDirectoryName).	self writeGlobalRoots.	str close.		! !!DlHtmlOutputter methodsFor: 'actions-output' stamp: 'M.U 5/3/1999 18:51'!outputOverview	| props str |	props := self properties.	str := self outStreamNamed: props overviewName, props htmlSuffix.	self writeOverview.	str close.	! !!DlHtmlOutputter methodsFor: 'actions-output' stamp: 'MU 6/25/2000 01:06'!outputPools	| props str nm |	props := self properties.	self analysisStorage getAllPools do: 		[:each | 		nm := each name.		str := self outStreamNamed: nm , props htmlSuffix in: (Array with: props poolDirectoryName).		self writePoolFor: each .		str close].	! !!DlHtmlOutputter methodsFor: 'actions-output' stamp: 'M.U 1/17/2000 00:32'!outputStylePage	| props str |	props := self properties.	str := self outStreamNamed: props styleSheetName in: (Array with: props styleSheetDirectoryName).	self writeStylePage.	str close.! !!DlHtmlOutputter methodsFor: 'actions-output' stamp: 'M.U 5/3/1999 18:52'!outputTopPage	| props str |	props := self properties.	str :=  self outStreamNamed: props topPageName, props htmlSuffix.	self writeTopPage.	str close.	! !!DlHtmlOutputter methodsFor: 'actions-output' stamp: 'MU 3/20/2000 00:05'!outputWholeIndex	| props cats str startIndex stopIndex pageAndAnalyzables |	props := self properties.	cats := self analysisStorage getAllAnalyzables.	startIndex := stopIndex := 0.	pageAndAnalyzables := OrderedCollection new.	[cats size < stopIndex]		whileFalse: 			[ | outs  pageName |			startIndex := stopIndex + 1.			stopIndex := startIndex + (props eachWholeIndexMax - 1).			outs := cats copyFrom: startIndex to: (stopIndex min: cats size).			outs isEmpty				ifFalse: 					[pageName := props generalIndexName , (stopIndex // props eachWholeIndexMax) printString , props htmlSuffix.					pageAndAnalyzables add: (Array							with: pageName							with: outs first							with: outs last).					str := self outStreamNamed: pageName in: (Array with: props wholeIndexDirectoryName).					self writeWholeIndexWith: outs].			str close].	str := self outStreamNamed: props wholeIndexName , props htmlSuffix in: (Array with: props wholeIndexDirectoryName).	self writeWholeIndexListWith: pageAndAnalyzables.	str close.! !!DlHtmlOutputter methods