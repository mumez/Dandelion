oc |	doc := self htmlDocFactory poolHtmlDoc.	doc analysisStorage: self analysisStorage.	doc pool: aDlAnalyzablePool.	doc writeOn: self outStream.! !!DlHtmlOutputter methodsFor: 'actions-page writing' stamp: 'M.U 1/17/2000 00:33'!writeStylePage	self htmlDocFactory styleSheetDoc writeOn: self outStream.			! !!DlHtmlOutputter methodsFor: 'actions-page writing' stamp: 'M.U 12/30/1999 17:32'!writeTopPage	self htmlDocFactory topHtmlDoc writeOn: self outStream.			! !!DlHtmlOutputter methodsFor: 'actions-page writing' stamp: 'MU 3/19/2000 02:32'!writeWholeIndexListWith: pageAndAnalyzables  	| doc |	doc := self htmlDocFactory wholeIndexListHtmlDoc.	doc pageAndAnalyzables: pageAndAnalyzables.	doc writeOn: self outStream.	! !!DlHtmlOutputter methodsFor: 'actions-page writing' stamp: 'M.U 12/31/1999 00:13'!writeWholeIndexWith: aCollectionOfDlAnalyzable  	| doc |	doc := self htmlDocFactory wholeIndexHtmlDoc.	doc analysisStorage: self analysisStorage.	doc analyzables: aCollectionOfDlAnalyzable.	doc writeOn: self outStream.! !!DlHtmlOutputter methodsFor: 'factory' stamp: 'MU 4/7/2000 18:32'!defaultActionSequence	^#(	#outputStylePage	#outputTopPage	#outputAllClassCategoryIndex	#outputAllClassIndex	#outputOverview	#outputClassCategories	#outputClasses	#outputGlobalRoots	#outputPools	#outputWholeIndex	).! !!DlHtmlOutputter methodsFor: 'factory' stamp: 'MU 2/13/2000 23:56'!defaultHtmlDocFactory	^DlHtmlDocFactory with: self properties.! !!DlHtmlOutputter methodsFor: 'accessing' stamp: 'M.U 12/30/1999 17:25'!htmlDocFactory	htmlDocFactory isNil ifTrue: [ htmlDocFactory := self defaultHtmlDocFactory].	^htmlDocFactory! !!DlHtmlOutputter methodsFor: 'accessing' stamp: 'M.U 12/30/1999 17:26'!htmlDocFactory: aValue	htmlDocFactory := aValue! !!DlHtmlOutputter methodsFor: 'converting' stamp: 'mu 1/3/2001 22:54'!asChangeSetOutputter	^self as: DlHtmlChangeSetOutputter! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!